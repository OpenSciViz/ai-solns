COMPONENT=astroh

VERSION=

HEACORE=heacore

HEATOOLS=heatools

ATTITUDE=attitude

CONFIGURE_CMD='cd $h_dir; ./configure --build=$build --with-heatop=$prefix --with-heatools=$h_config_heatools --enable-shared=$h_shared --enable-symbols=$h_symbols --enable-strip=$h_strip --enable-hera=$h_hera_build --x-libraries=$x_libraries --x-includes=$x_includes --with-copt="$h_copt" --with-cxxopt="$h_cxxopt" --with-fopt="$h_fopt" --enable-ldopt="$h_ldopt"'

DEPENDENCIES="$HEATOOLS $ATTITUDE"
